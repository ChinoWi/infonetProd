{
  "compilerOptions": {
    "baseUrl": "/node_modules/",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "DOM",
      "ES6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "paths": {
      "lodash": [
        "@types/lodash"
      ]
    },
    "pretty": true,
    "removeComments": false,
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5",
    "typeRoots": [
      "/node_modules/@types/"
    ],
    "types": [
      "node"
    ]
  }
}